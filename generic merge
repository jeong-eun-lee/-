#include <stdio.h>
#include <stdlib.h>

void merge(int *a, int m, int *b, int n, int *c)
{
    int i=0, j=0, k=0;

    while(i<m && j<n) {
        if(a[i]<b[j]) {
            c[k]= a[i];
        }
        else if(a[i]=b[j]) {
            c[k]= a[i]+b[j];
        }
        else {
            c[k]= b[j];
        }}
    if (i<m) {
    while (i<m) {
      c[k]=a[i];
      i++;
      k++;
    }}

    else{
     c[k]=b[j];
     j++;
     k++;
    }}

int main()
{
//int a={1,4,5,9,10};
 //int b={0,2,3,4,7,15,16};
 //int c= merge(a,10,b,6, c);
 //for(n=1; n<10; n++) {
 //printf("%d", c[n]);
 //}
    return 0;
}
